
{
  "manifest_version": 3,
  "name": "Smart Form Filler",
  "version": "1.0",
  "description": "Preenche formulários automaticamente com dados fictícios",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
